{
   "name": "security-microservice",
   "version": "1.0.0",
   "main": "src/server/server.ts",
   "license": "MIT",
   "scripts": {
      "dev": "ts-node-dev src/server/server.ts",
      "migration:generate": "typeorm-ts-node-commonjs -d ./src/config/AppDataBase.ts migration:generate ./src/models",
      "migration:run": "typeorm-ts-node-commonjs -d ./src/config/AppDataBase.ts migration:run",
      "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
      "typeor-mmigration": "yarn exec -- ts-node node_modules/typeorm/cli migration:revert -d ./src/config/AppDataBase.ts"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.0",
      "@types/express": "^4.17.14",
      "@types/jsonwebtoken": "^8.5.8",
      "@types/node": "^18.11.4",
      "@types/uuid": "^8.3.4",
      "nodemon": "^2.0.20",
      "sequelize-cli": "^6.4.1",
      "ts-node": "^10.9.1",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.1.0",
      "typescript": "^4.8.4"
   },
   "dependencies": {
      "bcrypt": "^5.0.1",
      "body-parser": "^1.19.1",
      "dayjs": "^1.11.5",
      "dotenv": "^16.0.1",
      "express": "^4.17.2",
      "jsonwebtoken": "^8.5.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.7",
      "uuid": "^8.3.2"
   }
}
